{
  "name": "market-1eca",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "postinstall": "remix setup node",
    "build": "run-s build:*",
    "build:remix": "remix build",
    "cypress:docker": "docker compose exec backend npx cypress install && cross-env PORT=8080 cypress run",
    "docker": "NODE_ENV=development docker-compose up -d",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks --inspect=0.0.0.0 -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "db:start": "docker compose up -d database",
    "db:stop": "docker compose down",
    "db:update": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:check": "docker ps"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@remix-run/server-runtime": "^1.5.1",
    "@stitches/react": "^1.2.8",
    "bcryptjs": "^2.4.3",
    "burner-email-providers": "^1.0.67",
    "date-fns": "^2.28.0",
    "downshift": "^6.1.7",
    "form-data": "^4.0.0",
    "mailgun.js": "^7.0.0",
    "marked": "^4.0.16",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "redis": "^4.1.0",
    "remix-auth": "^3.2.2",
    "remix-auth-email-link": "^1.3.1",
    "remix-auth-form": "^1.1.2",
    "remix-auth-socials": "^1.0.2",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.1.0",
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.2",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "cypress": "^9.7.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^4.1.0",
    "msw": "^0.41.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "prisma": "^3.14.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.12.9"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
